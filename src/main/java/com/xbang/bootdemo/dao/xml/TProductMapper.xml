<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xbang.bootdemo.dao.mapper.TProductMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xbang.bootdemo.dao.entity.TProduct">
        <id column="id" property="id" />
        <result column="product_name" property="productName" />
        <result column="product_desc" property="productDesc" />
        <result column="sales_price" property="salesPrice" />
        <result column="product_inventory" property="productInventory" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, product_name, product_desc, sales_price, product_inventory
    </sql>

</mapper>
